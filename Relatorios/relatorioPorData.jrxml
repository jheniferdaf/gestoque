	<queryString language="SQL">
		<![CDATA[SELECT movimentacao.codigo_produto,
	movimentacao.quantidade,
	movimentacao.cpf_usuario,
	movimentacao.tipo_movimentacao,
	movimentacao.data,
	movimentacao.observacao,
	produto.preco_venda,
	produto.preco_custo,
	produto.descricao
FROM movimentacao
	INNER JOIN produto ON 
	 movimentacao.codigo_produto = produto.codigo 
ORDER BY movimentacao.data DESC]]>
	</queryString>
